{
  "name": "fathom-sdk",
  "version": "0.1.53",
  "description": "Package with methods for formatting data and executing transactions on the Fathom protocol.",
  "scripts": {
    "build": "rm -r dist && tsc -p ./tsconfig.json && tsc -p ./tsconfig.json --module commonjs --outDir ./dist/cjs",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "author": "Fathom.fi",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/Into-the-Fathom/fathom-sdk.git"
  },
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.1.6"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "fathom-ethers": "^1.1.1",
    "eventemitter3": "^5.0.1"
  }
}
